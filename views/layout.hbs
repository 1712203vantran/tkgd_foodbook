                                    aria-label="search" aria-describedby="search dishes" 
                                    name="{{#if searchType}}{{searchType}}{{else}}dishName{{/if}}" 
                                    value="{{searchKeyword}}">
                                <div class="input-group-append">
                                    <button type="submit" id="searchDishesSubmitBtn" class="btn">
                                        <span class="input-group-text my-btn-bg-orange pl-3 pr-3">
                                            <i class="fas fa-search" aria-hidden="true"></i>
                                        </span>
                                    </button>
                                </div>
                            </form>
